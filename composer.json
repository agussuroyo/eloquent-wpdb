{
    "name": "agussuroyo/eloquent-wpdb",
    "description": "$wpdb that wrapped with Eloquent from Laravel",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Agussuroyo\\EloquentWpdb\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Agus Suroyo",
            "email": "jony.extenz@gmail.com"
        }
    ],
    "require": {
        "illuminate/database": "^8.83"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "cs": "./vendor/bin/phpcs",
        "stan": "./vendor/bin/phpstan analyse --memory-limit=-1",
        "checkAll": "composer cs && composer stan && composer test"
    }
}
